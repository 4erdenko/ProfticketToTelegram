# Core
IN_DOCKER=false
BOT_TOKEN=your_prod_bot_token
TEST_BOT_TOKEN=your_dev_bot_token
ADMIN_ID=123456789
ADMIN_USERNAME=admin

# Database (SQLAlchemy async URL)
DB_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/profticket
DB_ECHO=false
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=10

# Profticket
COM_ID=12345
PROXY_URL=
STOP_AFTER_ATTEMPT=5
WAIT_FIXED=3

# Bot behavior
MAINTENANCE=false
MAX_MSG_LEN=4069
TTL_IN_SEC=20
MAX_RATE_SEC_IN_TTL=10

# Show Update Service
UPDATE_INTERVAL=1800
ERROR_RETRY_INTERVAL=60
MAX_DATA_AGE=1800
MAX_CONSECUTIVE_ERRORS=3

# Timezone
DEFAULT_TIMEZONE=Europe/Moscow

# Optional (docker-compose)
POSTGRES_DB=profticket
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

